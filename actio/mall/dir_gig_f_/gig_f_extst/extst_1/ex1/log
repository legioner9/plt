$str =     ${_plt_exit} "fail: . {dir_fns_}/dir_{name_fn_}/.d.ax/.sal.ax/{name_fn_}.body.sh"
$str =     return 1
$str = fi
$str = cd "$PPWD" 1>/dev/null || plt_exit "fail : cd $PPWD"
