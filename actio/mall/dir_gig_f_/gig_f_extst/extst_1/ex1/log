$str = ARGS=($@)
$str = NARGS=$#
$str = verbose=0
$str = [[ " ${ARGS[*]} " =~ " -verbose " ]] || verbose=1
$str = [[ 1 -eq ${verbose} ]] || echo -e "${HLIGHT}---$filename $* ---${NORMAL}" #started functions
$str = idir=$(pwd)
$str = rdir="$(prs_f -d $filename)"
$str = gname="$(prs_f -n $filename)" # name without .ext
$str = wrp_fifs1_ cd  "$(prs_f -d $filename)" -d1
$str = g_args=($(garg2e_ "${ARGS[@]}"))
$str = [[ 1 -eq ${verbose} ]] || echo -e "${GREEN}\\${g_args[@]}: ${g_args[*]}${NORMAL}" #print variable
$str = for strex in $(garg2e_ "${ARGS[@]}"); do
$str =     [[ 1 -eq ${verbose} ]] || echo "local $strex"
$str =     eval $strex
$str = done
$str = _(){
$str =     echo "empty _deb in $filename"
$str =     # . $filename" arg1 arg2
$str = }
$str = if [ "_deb" == "$1" ]; then
$str =     _
$str =     return 0
$str = fi
