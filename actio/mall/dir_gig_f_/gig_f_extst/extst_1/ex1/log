$str = ARGS: 
$str = \$1
$str = [ ,\$2 num_menu ]
$str = CNTLS:
$str = required
$str = optional 
$str = CNTL inspect : -h, _man, _go, _list
$str = TAGS: (fs|net|)
$str = IFS: (fifs| exl| ...) - discribe in ${PATH_IFS_DIR}
$str = FLOW: (process | subprocess (no read pause only plt_err return \$errno) | interpritator)
$str = RETURN: ( result>stdout, return 0 | data | change to ptr |  fs_structure | ...)
$str = ERROR: ( (plt_err | plt_pause | plt_exit) errmes return 1 | ... )
$str = WARN: 
$str = DEBUG:
$str = EXAMP:
$str = ${FNN} -<>
$str = ${NORMAL}"
