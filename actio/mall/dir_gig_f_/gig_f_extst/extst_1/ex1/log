$str =         # echo -e "${GREEN}\$local_var_wrap = $local_var_wrap${NORMAL}" #print variable
$str =         eval local "$local_var_wrap"
$str =         local_var_wrap_arr+=("${local_var_wrap}")
$str =     done
$str =     if ! . "${PLT_PATH}/.d/.nid/{grad_}/{grad_}.const"; then
$str =         ${_plt_exit} "fail: . ${PLT_PATH}/.d/.nid/{grad_}/{grad_}.const"
$str =         return 1
$str =     fi
$str =     if ! . "${PLT_PATH}/.d/.nid/{grad_}/{grad_}.nid"; then
$str =         ${_plt_exit} "fail: . ${PLT_PATH}/.d/.nid/{grad_}/{grad_}.nid"
$str =         return 1
$str =     fi
$str =     echo -e "${HLIGHT}--- . ${PLT_PATH}/.d/.nid/{grad_}/{grad_}.nid, return 0 ---${NORMAL}" #start files
$str =     return 0
$str = fi
$str = # if ! ${_garg2e2_} "${ARGS[@]}" 1>/dev/null; then
$str = if ! ${_garg2e2_} "${ARGS[@]}"; then
$str =     ${_plt_exit} " {name_fn_} return 1: ${FNLOCK}"
$str =     return 1
$str = fi
$str = #? g_args with cntl=value
$str = local g_args=($(${_garg2e2_} "${ARGS[@]}"))
$str = #? e_args is {ARGS[@]} without cntl and cmtl_value - as it's free
$str = local e_args=($(${_earg2e2_} "${ARGS[@]}"))
$str = [[ 1 -eq ${verbose} ]] || echo -e "${GREEN}\${g_args[@]}: ${g_args[*]}${NORMAL}" #print variable
$str = local strex
$str = for strex in $(${_garg2e2_} "${ARGS[@]}"); do
$str =     [[ 1 -eq ${verbose} ]] || echo "local $strex"
$str =     eval local $strex
$str = done
$str = for local_var_fn in $(f2e "{dir_fns_}/dir_{name_fn_}/.d.ax/.sal.ax/{name_fn_}.local.list"); do
$str =     # echo -e "${GREEN}\$local_var_fn = $local_var_fn${NORMAL}" #print variable
$str =     eval local "$local_var_fn"
$str =     local_var_fn_arr+=("${local_var_fn}")
$str = done
$str = for local_var_wrap in $(f2e "${PLT_PATH}/.d/.nid/{grad_}/{grad_}.local"); do
$str =     # echo -e "${GREEN}\$local_var_wrap = $local_var_wrap${NORMAL}" #print variable
$str =     eval local "$local_var_wrap"
$str =     local_var_wrap_arr+=("${local_var_wrap}")
$str = done
$str = # echo ". ${PLT_PATH}/.d/.nid/mall.nid"
$str = if ! . "${PLT_PATH}/.d/.nid/{grad_}/{grad_}.const"; then
$str =     ${_plt_exit} "fail: . ${PLT_PATH}/.d/.nid/{grad_}/{grad_}.const"
$str =     return 1
$str = fi
