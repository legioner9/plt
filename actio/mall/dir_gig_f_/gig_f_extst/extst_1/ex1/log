$str = if [ "_man" == "$1" ]; then
$str =     edit_ "${d_name}/${FNN}".man
$str =     return 0
$str = fi
$str = if [ "_go" == "$1" ]; then
$str =     edit_ "${d_name}/${FNN}".sh
$str =     return 0
$str = fi
$str = if [ "_lst" == "$1" ]; then
$str =     edit_ "${d_name}/${FNN}".lst
$str =     return 0
$str = fi
$str = if [ "_head" == "$1" ]; then
$str =     echo "_head fn: ${d_name}/${FNN}"
$str =     return 0
$str = fi
$str = if ! garg2e_ "${ARGS[@]}" 1>/dev/null; then
$str =     plt_exit " ${FNN} return 1: ${FNLOCK}"
$str =     return 1
$str = fi
$str = g_args=($(garg2e_ "${ARGS[@]}"))
$str = [[ 1 -eq ${verbose} ]] || echo -e "${GREEN}\\${g_args[@]}: ${g_args[*]}${NORMAL}" #print variable
$str = for strex in $(garg2e_ "${ARGS[@]}"); do
$str =     [[ 1 -eq ${verbose} ]] || echo "local $strex"
$str =     eval local $strex
$str = done
