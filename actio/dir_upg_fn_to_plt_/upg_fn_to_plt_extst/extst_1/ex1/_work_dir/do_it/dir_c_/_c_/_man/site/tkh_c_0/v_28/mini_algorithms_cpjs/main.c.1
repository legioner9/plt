#define N 6

int factorial(int a)
{
    if (0 == a)
        return 1;

    int step = a * factorial(a - 1);
    // printf("a=%d, step=%d\n", a, step);
    return step;
}

int main(int argc, char const *argv[])
{
    int n;
    printf("enter int n for factorial:\n");
    scanf("%d", &n);
    printf("factorial = %d\n", factorial(n));
}
