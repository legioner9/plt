if [ "_lst" == "$1" ]; then
    [[ 1 -eq "${verbose}" ]] || echo -e "${HLIGHT}--- ${_edit_} ${FN_DIR}/${FNN}.lst ---${NORMAL}" #start files
    "${_wrp2_}" "${_edit_}" --_xxf "${FN_DIR}/${FNN}".lst
